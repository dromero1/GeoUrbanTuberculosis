<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
	<parameter name="randomSeed"
		displayName="Default Random Seed" type="int" defaultValue="__NULL__"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter name="averageRoomVentilationRate"
		displayName="Average room ventilation rate (AC/h)" type="double"
		defaultValue="3.0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="averageRoomVolume"
		displayName="Average room volume (m3)" type="double"
		defaultValue="75.0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="exposedCount" displayName="Exposed count"
		type="int" defaultValue="25" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter name="smokersShare"
		displayName="Smokers share (%)" type="double" defaultValue="0.0"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="meanDiagnosisDelay"
		displayName="Mean diagnosis delay (days)" type="double"
		defaultValue="45.0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="alcoholDrinkersShare"
		displayName="Alcohol drinkers share (%)" type="double"
		defaultValue="0.0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="immunodepressionShare"
		displayName="Immunodepression share (%)" type="double"
		defaultValue="0.0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="infectionProbability"
		displayName="Infection probability (%)" type="double"
		defaultValue="0.2" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="susceptibleCount"
		displayName="Susceptible count" type="int" defaultValue="5000"
		isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter" />
	<parameter name="treatmentDropoutRate"
		displayName="Treatment dropout rate (%)" type="double"
		defaultValue="0.0" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
	<parameter name="meanIncidenceRateGoal"
		displayName="Mean incidence rate goal (%)" type="double"
		defaultValue="0.5" isReadOnly="false"
		converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter" />
</parameters>